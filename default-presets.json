{
    "base": {
        "alignTo": 2,
        "preset": "veryslow",
        "report": true,
        "threads": 4,
        "peakSignalToNoiseRatio": true
    },

    "mpeg1": {
        "extends": "base",
        "videoCodec": "mpeg1video",
        "fps": "24",
        "bFrames":0,
        "strict": "-1",
        "format": "avi",
        "bitrate": "300k",
        "strict": -1

    },

    "mpeg4":  {
        "extends": "base",
        "videoCodec": "libx264",
        "audioCodec": "libfdk_aac",
        "fps": "24",
        "format": "mp4",
        "videoProfile": "main",
        "videoBitRate": "300k",
        "audioBitRate": "64k",
        "pixFormat": "yuv420p",
        "videoBufferSize": "720k"
    },

    "mpeg1TeaserVideo": {
        "makeStream": true,
        "extends": "mpeg1",
        "height": 56,
        "videoBitRate": "100k"
    },

    "aacAudio": {
        "makeStream": true,
        "format": "mp4",
        "videoCodec": null,
        "audioCodec": "libfaac",
        "audioBitRate": "64k"
    },

    "mpeg1LQVideo": {
        "makeStream": true,
        "extends": "mpeg4",
        "height": 128,
        "videoBitRate": "200k"
    },

    "mpeg1HQVideo": {
        "makeStream": true,
        "extends": "mpeg1",
        "height": 184,
        "videoBitRate": "300k"
    },

    "mpeg4LQ": {
        "makeStream": true,
        "extends": "mpeg4",
        "height": 320,
        "videoMaxBitRate": "180k",
        "videoBufferSize": "1200k"
    },

    "mpeg4LQVideo": {
        "makeStream": true,
        "extends": "mpeg4",
        "audioCodec": null,
        "height": 320,
        "videoMaxBitRate": "180k"
    },

    "mpeg4HQ": {
        "makeStream": true,
        "extends": "mpeg4",
        "height": 480,
        "videoProfile": "main",
        "videoMaxBitRate": "180k",
        "videoBufferSize": "1200k"
    },

    "mpeg4HQVideo": {
        "makeStream": true,
        "extends": "mpeg4",
        "audioCodec": null,
        "height": 480,
        "videoProfile": "main",
        "videoMaxBitRate": "180k"
    }


}
